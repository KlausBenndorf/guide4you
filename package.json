{
  "name": "guide4you",
  "version": "1.0.0",
  "description": "A configurable web client for geo-applications. Uses OpenLayers 3. Suitable for mobile devices.",
  "keywords": [
    "map",
    "map client",
    "openlayers"
  ],
  "author": "Klaus Benndorf",
  "homepage": "https://github.com/KlausBenndorf/guide4you",
  "license": "MIT",
  "contributors": [
    "Sascha Klemenjak <sascha.klemenjak@benndorf.de>",
    "Josef Schugt <josef.schugt@benndorf.de>",
    "Simon Seyock <simonseyock@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/KlausBenndorf/guide4you"
  },
  "dependencies": {
    "csscolorparser": "1.0.2",
    "guide4you-builder": "1.0.1",
    "guide4you-proxy": "github:KlausBenndorf/guide4you-proxy#v1.0.0",
    "iscroll": "5.2.0",
    "jquery": "3.1.0",
    "lodash": "4.14.1",
    "notifyjs-browser": "0.4.2",
    "openlayers": "github:KlausBenndorf/ol3#g4u3_3.16",
    "proj4": "2.3.14",
    "strip-json-comments": "2.0.1"
  },
  "devDependencies": {
    "concurrently": "2.2.0",
    "esdoc": "0.4.7",
    "mocha": "2.5.3",
    "pre-commit": "1.1.3",
    "selenium-webdriver": "2.53.3",
    "standard": "7.1.2",
    "wait-on": "1.5.2"
  },
  "scripts": {
    "build": "g4u-build -m prod -c",
    "dev": "g4u-build -m dev -c",
    "lint": "eslint src/",
    "pretest": "g4u-buildTests",
    "runTests": "npm run lint && wait-on -l http://localhost:8080 && mocha build/tests/*",
    "test": "concurrently --kill-others --success first \"npm run dev conf/full\" \"npm run runTests\" && echo \"Tests completed successfully\"",
    "doc": "esdoc -c esdoc.json"
  },
  "pre-commit": [
    "lint"
  ]
}
