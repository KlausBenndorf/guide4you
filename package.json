{
  "name": "guide4you-module-search",
  "version": "1.3.0",
  "description": "A search parser module for Guide4You.",
  "keywords": [
    "search",
    "map",
    "map client",
    "openlayers"
  ],
  "author": "Klaus Benndorf",
  "homepage": "https://github.com/KlausBenndorf/guide4you-module-search",
  "contributors": [
    "Sascha Klemenjak <sascha.klemenjak@benndorf.de>",
    "Josef Schugt <josef.schugt@benndorf.de>",
    "Simon Seyock <simonseyock@gmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KlausBenndorf/guide4you-module-search"
  },
  "peerDependencies": {
    "guide4you": "2.0.*",
    "jquery": "3.2.1",
    "openlayers": "github:KlausBenndorf/ol3#g4u3_v4.1.0"
  },
  "devDependencies": {
    "guide4you": "2.0.*",
    "jquery": "3.2.1",
    "openlayers": "github:KlausBenndorf/ol3#g4u3_v4.1.0",
    "guide4you-proxy": "github:KlausBenndorf/guide4you-proxy#v1.0.0",
    "guide4you-builder": "2.0.*",
    "concurrently": "^3.4.0",
    "esdoc": "0.5.2",
    "mocha": "3.4.2",
    "selenium-webdriver": "3.4.0",
    "phantomjs-prebuilt": "2.1.14",
    "standard": "10.0.2",
    "wait-on": "2.0.2"
  },
  "scripts": {
    "linkRepos": "g4u-link guide4you",
    "unlinkRepos": "g4u-unlink guide4you",
    "build": "g4u-build -m prod -c",
    "dev": "g4u-build -m dev -c",
    "lint": "eslint src/ tests/",
    "pretest": "g4u-buildTests",
    "runTests": "npm run lint && wait-on -l http-get://localhost:8080 && mocha build/tests/*",
    "test": "concurrently --kill-others --success first \"g4u-build -m dev -c conf\" \"npm run runTests\" && echo \"Tests completed successfully\"",
    "doc": "esdoc -c esdoc.json"
  }
}
